{
	"name": "react-basic-boilerplate2",
	"version": "1.0.0",
	"description": "A simple hello world template using Webpack4",
	"main": "src/index.js",
	"scripts": {
		"start": "webpack-dev-server --open",
		"prebuild": "rimraf dist",
		"build": "NODE_ENV='production' webpack -p"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rmemory/react-basic-boilerplate2.git"
	},
	"author": "Richard Memory",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/rmemory/react-basic-boilerplate2/issues"
	},
	"homepage": "https://github.com/rmemory/react-basic-boilerplate2#readme",
	"babel": {
		"presets": [
			"env",
			"react"
		],
		"plugins": [
			[
				"transform-class-properties",
				{
					"spec": true
				}
			]
		]
	},
	"dependencies": {
		"axios": "^0.18.0",
		"prop-types": "^15.6.2",
		"query-string": "^6.1.0",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"autoprefixer": "^9.1.1",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-loader": "^7.1.5",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"css-loader": "^1.0.0",
		"eslint": "^5.3.0",
		"eslint-config-airbnb": "^17.1.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^6.1.1",
		"eslint-plugin-react": "^7.11.1",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.1",
		"node-sass": "^4.9.3",
		"postcss-loader": "^3.0.0",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.22.1",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.5"
	}
}
